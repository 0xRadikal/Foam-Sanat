// Run `npx prisma migrate dev` after editing this schema.

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ContactMessage {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  message   String
  ip        String?
  userAgent String?
  createdAt DateTime @default(now())
}
